<?xml version="1.0"?>
<launch>
	<!-- global rosparam -->
	<param name="use_sim_time" value="true"/>

	<!-- rosbag -->
	<node pkg="rosbag" type="play" name="player" args="--clock $(find localizer)/example_data/d_kan_indoor.bag"/>

	<!-- rviz -->
	<node pkg="rviz" type="rviz" name="rviz" args="-d $(find localizer)/config/local.rviz"/>
	
	<!-- main -->
	<include file="$(find localizer)/launch/ekf.launch"/>
	<include file="$(find localizer)/launch/map_match.launch"/>
	<include file="$(find localizer)/launch/tf_pub.launch"/>
</launch>
